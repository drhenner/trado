<td class="<%= ('triple-menu' if delivery_service_record) %> table-actions">
  <% if show %>
    <%= link_to object do %>
      <i class="icon-search"></i>
    <% end %>
  <% end %>
  <% if delivery_service_record %>
    <%= link_to admin_delivery_service_delivery_service_prices_path(object.last) do %>
      <i class="icon-search"></i>
    <% end %>
  <% end %>
  <% if edit %>
    <%= link_to edit_polymorphic_path(object) do %>
      <i class="icon-pencil"></i>
    <% end %>
  <% end %>
  <% if remote_edit %>
    <a href="#" data-record-id="<%= object.last.id %>" class="edit-<%= Store.class_name_id(object.last) %>-record">
      <i class="icon-pencil"></i>
    </a>
  <% end %>
  <% if order_record && object.last.latest_transaction.completed? && object.last.pending? %>
    <a href="#" data-order-id="<%= object.last.id %>" id="dispatcher">
      <i class="icon-airplane"></i>
    </a>
    <%= link_to receipt_admin_order_path(object.last) do %>
      <i class="icon-file"></i>
    <% end %>
  <% end %>
  <% if remote_delete %>
    <a href="#" data-url="<%= polymorphic_path(object) %>" class="<%= Store.class_name_id(object.last) %>-delete">
      <i class="icon-remove"></i>
    </a>
  <% end %>
  <% if del %>
    <%= link_to object, method: :delete, remote: remote_delete, data: { confirm: 'Are you sure?' } do %>
      <i class="icon-remove"></i>
    <% end %>
  <% end %>
</td>