- content_for :title, 'New accessory'
- content_for :breadcrumb, 'New'
- breadcrumb_add 'Accessories', admin_accessories_path

.twelvecol
  .page-header
    %h2.widget.widget-header New accessory
  = form_for [:admin, @accessory], html: { role: 'form' } do |f|
    .threecol
    .ninecol.last
      = render partial: 'shared/errors', locals: { object: @accessory }
    = render partial: 'form', locals: { f: f }
.fourcol.last
        

