  / ==========
  / = Most Popular products =
  / ==========
  .most-popular.blocks-spacer
    .container
      / ==========
      / = Title =
      / ==========
      .row
        .span12
          .main-titles.lined
            %h2.title
              %span.light Featured
              Products
      / /title
      .row.popup-products
        - @featured_products.each do |product|
          .span3
            .product
              .product-inner
                .product-img
                  .picture
                    = link_to (image_tag product.attachments.first.file.large, :alt => product.name, :height => 412, :width => 540), [product.category, product]
                .main-titles.no-margin
                  %h4.title
                    = Store::Price.new(product.skus.first.price, true).markup
                  %h5.no-margin= truncate product.name, :length => 35
                %p.desc= truncate product.short_description, :length => 100
